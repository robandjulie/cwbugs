
@import url(http://fonts.googleapis.com/css?family=Roboto:400,300,500);

/* FONT FACE */
@font-face {
    font-family: 'Pathway Gothic One';
    src: url('fonts/pathwaygothicone-regular-webfont.eot');
    src: url('fonts/pathwaygothicone-regular-webfont.svg#pathway_gothic_oneregular') format('svg'),
    	 url('fonts/pathwaygothicone-regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('fonts/pathwaygothicone-regular-webfont.woff') format('woff');
         /* url('fonts/pathwaygothicone-regular-webfont.ttf') format('truetype'), */
    font-weight: normal;
    font-style: normal;
}

/* BODY */
body {
	margin-top: 32px;
}

/* HEADINGS */
h1, h2, h3, h4, h5, h6,
.h1, .h2, .h3, .h4, .h5, .h6 {
	/* font-family: 'Roboto', Helvetica, Arial, sans-serif; */
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-weight: 500;
}

/* TWO-COLUMN CSS */
.outer-container.container {
	clear: left;
	float: left;
	width: 1173px;
	padding: 0px;
	overflow: hidden;
	background-color: #ef6a25;
}

.inner-container {
	float: left;
	width: 100%;
	position: relative;
	left: 250px;
	background-color: #fff;
	
}
#navigation {
	float: left;
	width: 250px;
	position: relative;
	left: -250px;
}

#content-well {
	float: left;
	position: relative;
	left: -250px;
	margin-bottom: 30px;
	overflow: hidden;
}

body.front #content-well {
	left: -295px;
}

/* LEFT COLUMN MENU */
#navigation.col-sm-3 {
	margin: 0px;
	padding: 0px;
	width: 250px;
	float: left;
}

.region.region-sidebar-first {
	padding: 0px;
	margin: 0px 0px 15px 0px;
	background-color: #ef6a25;
	border: none;
	border-radius: 0px;
}

#logo {
	height: 134px;
	width: 270px;
	position: absolute;
	top: 20px;
	left: 0px;
	z-index: 10;
}

#block-superfish-1 {
	margin-top: 151px;
	width: 250px;
}

#block-superfish-1 .sf-vertical li:hover > ul, 
#block-superfish-1 .sf-vertical li.sfHover > ul {
	left: 14.5em;
}

ul#superfish-1 * {
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-size: 17px;	
	background-color: #ef6a25;
	color: #fff;
	letter-spacing: .2px;
}

ul#superfish-1 > li {
	padding-left: 24px;
	width: 250px;
}

ul#superfish-1 li:first-child {
	border-top: none;
}

ul#superfish-1 li:last-child {
	border-bottom: none;
}

ul#superfish-1 li {
	padding-top: 6px;
	padding-bottom: 6px;
	border-bottom: 1px solid #ea5d1e;
	border-top: 2px solid #f9753b;
}

ul#superfish-1 li a:after {
	content: '> ';
	float: right;
	margin-right: 10px;
}

ul#superfish-1 li.sf-no-children a:after {
	content: none;
}

div.interact {
	margin: 25px 26px;
}

a#donate-button, a#buy-tickets-register-button,
a#donate-button:hover, a#buy-tickets-register-button:hover,
.blue-button, .blue-button:hover, 
.field-name-field-footer-link a, .field-name-field-footer-link a:hover {
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	text-transform: uppercase;
	color: #fff;
	width: auto;
	display: block;
	text-align: center;
	padding: 5px 0px;
	font-size: 25px;
	text-decoration: none;
}

a#donate-button,
.blue-button, 
.field-name-field-footer-link a {
	background-color: #8cdde0;
	border-bottom: 1px solid #39b9be;
	text-shadow: 1px 1px 1px #39b9be;
	margin-bottom: 4px;
	color: #fff !important;

}

a#buy-tickets-register-button {
	background-color: #d75513;
	border-bottom: 1px #c14708;
	text-shadow: 1px 1px 1px #c14708;
}

div.newsletter {
	text-align: center;
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	color: #fff;
	text-transform: uppercase;
	display: block;
	margin-top: 23px;
	font-size: 20px;
}

div.newsletter input[type="text"] {
	font-family: Helvetica, Arial, sans-serif;
	width: 100%;
	border: 3px solid #fff;
	background-color: transparent;
	margin-bottom: 4px;
	padding: 5px;
	color: #fff;
	font-size: 14px;
}

div.newsletter input[type="submit"] {
	border: 0px;
	color: #fff;
	width: 100%;
	background-color: #d75513;
	border-bottom: 1px #c14708;
	text-shadow: 1px 1px 1px #c14708;
	text-transform: uppercase;
	font-size: 25px;
}

div.social-media {
	text-align: center;
}

div.social-media a,
div.social-media a:hover {
	background-image: url(../images/social-icons.png);
	height: 39px;
	width: 39px;
	display: block;
	float: left;
	margin-right: 10px;
	text-decoration: none;
	
	background-repeat: no-repeat;
}

div.social-media a.facebook {
	background-position: -4px -3px;
}

div.social-media a.twitter {
	background-position: -55px -3px;
}

div.social-media a.vimeo {
	background-position: -157px -3px;
}

/* PANES */
.panels-flexible-column.panels-flexible-column-last {
	padding-right: none !important;
}

.panels-flexible-6 .panels-flexible-column {
	padding-right: 30px;
}

.cf-box {
	width: 33%;
	float: left;
	margin-right: 2%;
}

.fieldable-panels-pane,
.cf-box {
	border: 2px solid #8cdde0;
}

.panels-flexible-column-inside {
	padding: 0px !important;
}

.pane-title {
	text-transform: uppercase;
	color: #ef6a25;
}


.fieldable-panels-pane .field-name-field-pane-title,
.fieldable-panels-pane .field-name-field-body, 
.cf-box-title,
.cf-box-body {
	margin: 0px 20px 20px;
}

.fieldable-panels-pane .field-name-field-image,
.cf-box-image-wrapper {
	width: 100%;
}

.fieldable-panels-pane .field-name-field-image img,
.cf-box-image {
	margin-left: -17px;
	margin-top: -24px;
}

.cf-box-image img {
	width: 100%;
	height: auto;
}

.cf-box-title,
.cf-box-title a,
.fieldable-panels-pane .field-name-field-pane-title {
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	color: #ef6a25;
	margin-top: 20px;
	font-size: 30px;
	text-transform: uppercase;
}

.cf-box-link,
.fieldable-panels-pane .field-name-field-internal-link {
	background-color: #8cdde0;
	margin-left: 20px;
	margin-top: 30px;
	margin-bottom: -15px;
	text-align: center;
	width: 100%;
	padding: 5px 0px;
	border-bottom: 1px solid #39b9be;
}

.cf-box-link a,
.cf-box-link a:hover,
.fieldable-panels-pane .field-name-field-internal-link a,
.fieldable-panels-pane .field-name-field-internal-link a:hover {
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-size: 25px;
	text-transform: uppercase;
	text-decoration: none;
	color: #fff;
}

.cf-box-link a:before,
.fieldable-panels-pane .field-name-field-internal-link a:before {
	content: "+ ";
}



/* MAIN CONTENT */
#content-well {
	width: 922px;
	padding-right: 0px;

}

.row {
	margin: 0px;
}

.region-content {
	margin-left: 20px;
	padding-top: 10px;
	font-family: 'Roboto', Helvetica, Arial, sans-serif;
	font-weight: 300;
	color: #475455;
}

.region-content a {
	color: #ef6a25;
}


.panel-pane img {
	width: inherit;
	max-width: 100%;
	height: auto;
}

/* HOMEPAGE SLIDESHOW */

#homepage-slideshow {
	margin-left: -35px;
	width: 923px;
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	clear: both;
	margin-bottom: 100px;
}

#homepage-slideshow .views-field-field-carousel-image, 
#homepage-slideshow .views-field-field-carousel-image * {
	z-index: -10000;
}

#homepage-slideshow .views-field-title {
	text-transform: uppercase;
	letter-spacing: 1px;
	background-color: #8cdde0;
	float: left;
	font-size: 52px;
	padding: 0px 12px 3px 30px;
	position: relative;
}

#homepage-slideshow .views-field-title a,
#homepage-slideshow .views-field-title a:hover {
	color: #fff;
	text-decoration: none;
}

#homepage-slideshow .views-field-field-secondary-copy {
	float: left;
	padding: 5px 10px 8px 30px;
	background-color: #ef6a25;
	color: #fff;
	font-size: 40px;
	margin-top: 15px;
	margin-left: -17px;
	padding-right: 10px;
}


#homepage-slideshow .homepage_slideshow_titles_outer_wrapper {
	position: absolute;
	top: 285px;
	left: 20px;
}

/* FOOTER */
footer.footer.container {
	border: none;
	padding: 0px;
	margin: 0px;
	background-color: #3E4346;
	width: 1173px;
}

div.sponsors-header {
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	text-transform: uppercase;
	color: #EAEEEF;
	margin-top: 20px;
	margin-bottom: 20px;
	text-align: center;
	font-size: 21px;
}

div.view.view-sponsors.view-display-id-block {
	width: 80%;
	margin: 20px auto 20px auto;
}

div.view.view-sponsors.view-display-id-block .views-row {
	width: 20%;
	float: left;
	padding: 0px 15px 0px;
}

div.view.view-sponsors.view-display-id-block .views-row img {
	width: inherit;
	max-width: 100%;
	height: auto;
}

#block-menu-block-1 {
	padding-top: 40px;
	padding-bottom: 40px;
}

.menu-block-1.menu-name-main-menu * {
	padding: 0px;
	margin: 0px;
	display: block;
}

.menu-block-1.menu-name-main-menu > ul {
	width: 80%;
	margin-left: auto;
	margin-right: auto;
}

.menu-block-1.menu-name-main-menu > ul > li {
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	color: #94DDE4;
	font-size: 16px;
	width: 20%;	
	float: left;
	padding-right: 30px;
	margin-bottom: 30px;
	/* border: 1px dotted orange; */
} 

.menu-block-1.menu-name-main-menu > ul > li a,
.menu-block-1.menu-name-main-menu > ul > li a:hover {
	text-decoration: none;
	color: #94DDE4;
}

.menu-block-1.menu-name-main-menu > ul > li > ul {
	font-family: Geneva, Tahoma, Verdana, sans-serif;
	color: #ddd;
	font-size: 11px;
	margin-top: 6px;
	/* border: 1px dotted green; */
}

.menu-block-1.menu-name-main-menu > ul > li > ul a,
.menu-block-1.menu-name-main-menu > ul > li > ul a:hover {
	text-decoration: none;
	color: #ddd;
}

.menu-block-1.menu-name-main-menu > ul > li > ul > li > ul {
	font-family: Geneva, Tahoma, Verdana, sans-serif;
	color: #999;
	padding-left: 10px;
}

/* SUBPAGES */

body.not-front .content-header {
	background-color: #8cdde0;
	padding-left: 50px;
	padding-bottom: 40px;
	min-height: 153px;
}

body.not-front #content-well {
	padding: 0px;
}

h1.page-header {
	border: 0px;
	padding: 0px 0px 0px 15px;
	margin: 20px 0px 0px 0px;
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	color: #fff;
	font-size: 65px;
	text-transform: uppercase;
}

h2.director {
	border: 0px;
	padding: 0px 0px 0px 15px;
	margin: 0px;
	font-family: Geneva, Tahoma, Verdana, sans-serif;
	color: #fff;
	font-size: 15px;
}

.breadcrumb {
	font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
	padding: 0px;
	margin: 0px;
	border-radius: 0px;
	background-color: transparent;
}

.breadcrumb .breadcrumb-inner {
	padding: 15px 10px 10px;
	background-color: #fff;
}

.breadcrumb .breadcrumb-inner,
.breadcrumb .breadcrumb-inner *,
.breadcrumb .breadcrumb-inner a {
	font-size: 11px;
	color: #999;
	text-decoration: none;	
}

.region.region-content {
