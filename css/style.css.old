}

/* SCREENING */

body.node-type-screening .group-right {
	padding-left: 15px;
}

body.node-type-screening .group-footer {
	padding-top: 50px;
}

#screening-video {
	width: 488px;
	float: left;
}

#screening-details {
	width: 309px;
	float: left;
}

#screening-details .field {
	font-size: .9em;
	color: #999;
	padding-left: 35px;
	padding-bottom: 17px;
}

#screening-details .field.screening-details-footer {
	color: #000;
	font-weight: bold;
	font-style: italic;
}

#screening-dates {
	width: 100%;
	float: left;
	font-size: .9em;
	color: #999;	
	margin-top: 80px;
}

#screening-dates .field-collection-container {
	border-bottom: none;
}

.field-name-field-screening-dates .field-item {
	border: 1px solid #8cdde0;
	padding-top: 25px;
	padding-right: 25px;
	width: 42%;
	float: left;
}

.field-name-field-screening-dates .field-item.even {
	margin-left: 37px;
}

.field-name-field-screening-dates .field-item.odd {
	margin-left: 75px;
}


.field-name-field-screening-dates .screening-date-wrapper {
	position: relative;
}

.field-name-field-screening-dates .screening-date {
	width: 130px;
	height: 130px;
	background-color: #ef6a25;
	color: #fff;
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	margin: -45px 25px 18px -25px;
	text-align: center;
	float: left;
}

.field-name-field-screening-dates.field-type-field-collection .field-collection-view {
	margin: 0px;
	padding: 0px;
}

.field-name-field-screening-dates .screening-location {
	margin-bottom: 30px;
}

.field-name-field-screening-dates .screening-date .month {
	font-size: 3em;
	text-transform: uppercase;
	margin-top: 4px;
}
.field-name-field-screening-dates .screening-date .day {
	font-size: 6em;
	margin-top: -31px;
	margin-left: -3px;
}

.field-name-field-screening-dates .screening-tickets a {
	position: absolute;
	bottom: 0px;
	right: 0px;
	margin-bottom: -17px;
	margin-right: -47px;
	padding: 4px 51px;
}

/* CALENDAR */
body.page-calendar .calendar-calendar .month-view thead * {
	border: 1px solid #ef6a25;
}
body.page-calendar .calendar-calendar .month-view th.days {
	background-color: #ef6a25;
	color: #fff;
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	text-align: center;
	font-size: 1.5em;
	text-transform: uppercase;
}

body.page-calendar .calendar-calendar tbody td {
	border-width: 1px 1px 1px 1px;
	border-color: #8cdde0;
}

body.page-calendar .calendar-calendar .month-view .full td.date-box.today {
	border-width: 1px 1px 0px 1px;
	border-color: #8cdde0
}

body.page-calendar .calendar-calendar .month-view .full td.today,
body.page-calendar .calendar-calendar .month-view .full td.today * {
	background-color: #8cdde0;
	color: #fff;
}

body.page-calendar .calendar-calendar .month-view .full tr td.today {
	border-bottom: 1px solid #8cdde0;
	border-left: 1px solid #8cdde0;
	border-right: 1px solid #8cdde0;
}

body.page-calendar .calendar-calendar td.empty {
	color: #999;
}

body.page-calendar .calendar-calendar td .inner {
	float: left;
	font-size: 0.9em;
}

.view-calendar .pager {
	margin: 0px;
}

.view-calendar .pager * {
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
	background-color: none !important;
	border: 0px !important;
	margin: 0px !important;
	padding: 0px !important;
}

.view-calendar .date-heading h3 {
	float: left;
	font-family: 'Pathway Gothic One', "Futura-CondensedMedium", "HelveticaNeue-Condensed", "Helvetica Neue", Helvetica, Arial, sans-serif;
}

.view-calendar .date-nav {
	padding-bottom: 0px;
	margin-bottom: 0px;
}

